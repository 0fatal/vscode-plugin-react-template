{
    "name": "vscode-plugin-react-template",
    "displayName": "vscode-plugin-react-template",
    "description": "",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-plugin-react-template.helloWorld"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-plugin-react-template.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "build:view": "webpack --config webpack.view.config.js",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p -w . --outDir out",
        "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/react": "^17.0.40",
        "@types/react-dom": "^17.0.13",
        "@types/vscode": "^1.65.0",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vscode/test-electron": "^2.1.2",
        "autoprefixer": "^10.4.2",
        "css-loader": "^6.7.1",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mini-css-extract-plugin": "^2.6.0",
        "mocha": "^9.2.1",
        "postcss": "^8.4.8",
        "postcss-loader": "^6.2.1",
        "tailwindcss": "^3.0.23",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
